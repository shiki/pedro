{
  "name": "Pedro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "start:ios": "concurrently -r 'react-native start' 'react-native run-ios'",
    "test": "jest --setupTestFrameworkScriptFile=./src/__tests__/init.js src",
    "it": "jest --setupTestFrameworkScriptFile=./integrationTests/lib/init.js integrationTests/lib",
    "it:build": "detox build"
  },
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "crypto-js": "^3.1.9-1",
    "moment": "^2.18.1",
    "offensive": "^0.8.0",
    "query-string": "^5.0.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.48.4",
    "react-native-config": "^0.9.0",
    "react-native-navigation": "latest",
    "react-native-sqlite-storage": "^3.3.3",
    "react-native-uuid-generator": "^2.0.0",
    "react-native-vector-icons": "^4.3.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-react-native": "2.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "detox": "^5.9.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^21.2.1",
    "mock-async-storage": "^2.0.2",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-logic-test": "^1.0.5"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": ["<rootDir>/node_modules", "<rootDir>/src/__tests__/init.js"]
  },
  "detox": {
    "runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Pedro.app",
        "build": "xcodebuild -project ios/Pedro.xcodeproj -scheme PedroIntegration -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
